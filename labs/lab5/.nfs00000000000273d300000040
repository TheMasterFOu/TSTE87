-- Electronics Systems, http://www.es.isy.liu.se/

library ieee;
use ieee.std_logic_1164.all;

entity pecontroller2 is
port(
state : in integer range 0 to 3;
coefficient : out std_logic_vector(0 to 15);
start : out std_logic);
end pecontroller2;

architecture generated of pecontroller2 is
begin
with state select
coefficient <= 
"0100000000000000" when 0,
"0100000000000000" when 2,
"----------------" when others;
with state select
start <= 
'1' when 0,
'1' when 2,
'0' when others;

end generated;
